[credential]
    helper = store
[pull]
    rebase = false
[diff]
    submodule = log
[status]
    submoduleSummary = true
[alias]
    spush = push --recurse-submodules=on-demand
    addignore = "!f() { echo $1 >> .gitignore; }; f"
    untrack = !git update-index --assume-unchanged
    track = !git update-index --no-assume-unchanged
