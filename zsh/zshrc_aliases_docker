alias dstart="docker-compose -f docker-compose-dev.yml up"
alias ddstart="docker-compose -f docker-compose-dev.yml up -d"
alias dlist="docker-compose -f docker-compose-dev.yml ps"
alias dstop="docker-compose -f docker-compose-dev.yml stop"
alias drstop="dstop && docker-compose -f docker-compose-dev.yml rm -f"
alias dbuild="drstop && docker-compose -f docker-compose-dev.yml build && dstart"
alias ddbuild="drstop && docker-compose -f docker-compose-dev.yml build && ddstart"
alias dssh="ddstart && dockercomposessh"
